{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.7.1",
      "destination": "wwwroot/jquery/"
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/jquery-validation-unobtrusive/"
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/jquery-validate/",
      "files": [
        "jquery.validate.min.js",
        "localization/messages_ru.js",
        "localization/messages_ru.min.js",
        "jquery.validate.js",
        "additional-methods.js",
        "additional-methods.min.js"
      ]
    },
    {
      "library": "bootstrap-icons@1.11.1",
      "destination": "wwwroot/bootstrap/bootstrap-icons/"
    },
    {
      "library": "bootstrap@5.3.2",
      "destination": "wwwroot/bootstrap/bootstrap/"
    }
  ]
}